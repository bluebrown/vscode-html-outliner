{
	"name": "outliner",
	"displayName": "outliner",
	"description": "outline html docs like the browser",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:outliner.outline",
		"onLanguage:html"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "outliner.outline",
				"title": "Outline",
				"when": "editorLangId == html"
			},
			{
				"command": "outliner.refresh",
        "title": "Refresh"
			}
		],
		"views": {
      "explorer": [
        {
          "id": "documentOutline",
          "name": "Document Outline"
        }
      ]
    }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"h5o": "^0.11.3",
		"jsdom": "^16.2.2"
	}
}
